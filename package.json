{
  "name": "webpack-babel",
  "version": "1.0.0",
  "description": "a vue-express-vuecli3 project",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "set NODE_ENV=development&&nodemon src/server/index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "set NODE_ENV=production&&node src/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biubiupiu1/vue-express-vuecli3-template.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "vue-cli3",
    "vue",
    "express"
  ],
  "author": "biubiupiu",
  "license": "MIT",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "express": "^4.16.3",
    "module": "^1.2.5",
    "nodemon": "^1.18.4",
    "readline": "^1.3.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
